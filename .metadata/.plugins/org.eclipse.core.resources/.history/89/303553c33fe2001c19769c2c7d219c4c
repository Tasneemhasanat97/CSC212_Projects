
public class Lab3 {

	public static void main(String[] args) { // String args[]
	
		
		
		float f, c; 	// declare public variables 
		
		f = (float) 98.6;	// assign variable value for f 
		
		c = (5/9)*(f-32);		// conversion f to c
		
		
		
		System.out.println(f); // equivalent to cout << endl;
		
		
		
	}

}
